script.
    
  function AjaxFormRequest(result_id,formMain,url) {
  if (document.getElementById(formMain).checkValidity()) {
  jQuery.ajax({
  url: url,
  type: "POST",
  dataType: "html",
  data: jQuery("#"+formMain).serialize(),
  success: function(response) {
  document.getElementById(result_id).innerHTML = response;
  },
  error: function(response) {
  document.getElementById(result_id).innerHTML = "<span class='error'>Возникла ошибка при отправке формы. Попробуйте еще раз</error>";
  
  }
  });
  $(':input','#formMain').not(':button, :submit, :reset, :hidden').val('');
  $('#formMain .holder-elemnt').css({position:'absolute', left:'-9999px'});
  } 
  }

